{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "watch": "npm-watch docs",
    "start": "npm-run-all --parallel lint-watch start-dev",
    "start-dev": "node ./node_modules/.bin/expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android --generate-keystore --non-interactive > output.txt",
    "build:ios": "expo build:ios",
    "eject": "expo eject",
    "docs": "./node_modules/.bin/esdoc",
    "lint": "node_modules/.bin/esw",
    "lint-watch": "node_modules/.bin/esw --watch",
    "lint-test": "npm-run-all --parallel lint-watch test",
    "prettier": "prettier --write '**/*.js'",
    "test": "jest --watchAll",
    "publish-app": "node ./node_modules/.bin/expo publish",
    "download-build": "tail -n 1 output.txt | head -n 1 | cut -c47- | xargs wget -O build.apk"
  },
  "jest": {
    "preset": "jest-expo",
    "testRegex": "./tests/.*.test.js$"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "~10.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-native-fontawesome": "^0.2.5",
    "@react-native-community/masked-view": "0.1.5",
    "@react-native-community/picker": "^1.7.1",
    "@react-navigation/bottom-tabs": "^5.0.0",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "@react-navigation/web": "~1.0.0-alpha.9",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.2",
    "buffer": "^5.5.0",
    "expo": "~36.0.0",
    "expo-asset": "~8.0.0",
    "expo-constants": "~8.0.0",
    "expo-crypto": "^8.0.0",
    "expo-file-system": "^8.0.0",
    "expo-font": "~8.0.0",
    "expo-image-picker": "^8.1.0",
    "expo-location": "^8.0.0",
    "expo-mail-composer": "^8.1.0",
    "expo-permissions": "^8.1.0",
    "expo-web-browser": "~8.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mobx": "^5.14.0",
    "mobx-react": "5.4.4",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-easy-toast": "^1.2.0",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-markdown-display": "^6.1.4",
    "react-native-picker-select": "^8.0.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-svg": "9.13.3",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-expo": "~8.0.0",
    "detox": "^16.0.2",
    "esdoc": "^1.1.0",
    "esdoc-custom-theme": "^1.4.2",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-publish-html-plugin": "^1.1.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^6.0.1",
    "jest-expo": "~36.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "prettier": "1.18.2"
  },
  "private": true
}
